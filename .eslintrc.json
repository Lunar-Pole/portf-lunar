{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off"
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    }
}
